   "source": [
    "# Write a Python function to sum all the numbers in a list.\n",
    "Sample List : (8, 2, 3, 0, 7) Expected Output : 20\n",
    "\n",
    "Explanation:\n",
    "\n",
    "Summation should like 8+2+3+0+7 = 20"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "b868ea3d",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "20\n"
     ]
    }
   ],
   "source": [
    "list1=(8,2,3,0,7)\n",
    "def sum(list1):\n",
    "    sum1=0\n",
    "    for i in list1:\n",
    "        sum1=sum1+i\n",
    "    return sum1\n",
    "print(sum(list1))"
   ]
  }
